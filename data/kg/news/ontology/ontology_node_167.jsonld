                        },
                        "mentions_organizations": {
                            "@type": "OrganizationList",
                            "Organization": [
                                {
                                    "name": "The Independent",
                                    "hasLocation": "London, UK"
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}