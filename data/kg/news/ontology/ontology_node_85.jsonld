                            "Organization": [
                                {
                                    "name": "Sporting News",
                                    "hasLocation": {
                                        "@id": "geo:SpatialThing",
                                        "name": "USA"
                                    },
                                    "alias": "SN"
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}