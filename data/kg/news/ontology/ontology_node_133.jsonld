                        "theme": "Exploration",
                        "hasLocation": {
                            "@id": "geo:SpatialThing",
                            "name": "The Depths"
                        },
                        "mentions_persons": {
                            "@id": "snap:personList",
                            "Person": []
                        },
                        "mentions_organizations": {
                            "@id": "snap:organizationList",
                            "Organization": []
                        }
                    },
                    {
                        "@id": "snap:event",
                        "eventTitle": "Realization of Princess Zelda's Location",
                        "eventSummary": "The moment of understanding where Princess Zelda really was.",
                        "theme": "Narrative",
                        "hasLocation": {
                            "@id": "geo:SpatialThing",
                            "name": "Hyrule"
                        },
                        "mentions_persons": {
                            "@id": "snap:personList",
                            "Person": []
                        },
                        "mentions_organizations": {
                            "@id": "snap:organizationList",
                            "Organization": []
                        }
                    }
                ]
            },
            "followUp": "snap:asset",
            "derivedFrom": "snap:asset"
        }
    ]
}