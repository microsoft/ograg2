            "created": "2023-10-01T00:00:00Z",
            "modified": "2023-10-01T00:00:00Z",
            "published": "2023-10-01T00:00:00Z",
            "has_events": {
                "@id": "snap:eventList",
                "Event": [
                    {
                        "@id": "snap:event",
                        "eventTitle": "First Dive into the Depths",
