                        "theme": "sports betting",
                        "mentions_persons": {
                            "@id": "snap:personList",
                            "Person": []
                        },
                        "mentions_organizations": {
                            "@id": "snap:organizationList",
                            "Organization": [
                                {
                                    "@id": "snap:organization",
                                    "name": "Sporting News"
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}