            "content": "Amazonâ€™s fall Prime Day event is underway. For more deals we recommend across all categories, check out our full roundup here.",
            "published": "2023-10-10T12:30:00+00:00",
            "has_events": [
                {
                    "@id": "snap:event1",
                    "@type": "schema:Thing",
                    "eventTitle": "Amazon Prime Day Gaming Deals",
                    "eventSummary": "Amazon Prime Day is one of the best times of the year to save on a wide variety of video games.",
                    "theme": "shopping",
                    "mentions_persons": [],
                    "mentions_organizations": [
                        {
                            "@id": "snap:organization1",
                            "@type": "schema:Thing",
                            "name": "Amazon"
                        }
                    ]
                }
            ]
        }
    ]
}