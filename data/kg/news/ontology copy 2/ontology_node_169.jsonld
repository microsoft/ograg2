                    },
                    "mentions_persons": [
                        {
                            "@id": "snap:person1",
                            "@type": "snap:person",
                            "name": "Mark Zuckerberg",
                            "alias": "Zuck"
                        }
                    ],
                    "mentions_organizations": [
                        {
                            "@id": "snap:organization1",
                            "@type": "snap:organization",
                            "name": "Meta",
                            "alias": "Facebook"
                        }
                    ]
                }
            ]
        }
    ]
}