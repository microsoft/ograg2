            "@context": {
                "name": "schema:Text",
                "quantity": "schema:Text",
                "stage": "schema:Text"
            }
        },
        "SoilRequirements": {
            "@id": "cropCult:SoilRequirements",
            "@type": "schema:Thing",
            "@context": {
                "soil_type_name": "schema:Text",
                "avoid_soil_type": "schema:Text",
                "for_water": "schema:Text",
                "for_nutrients": "schema:Text"
            }
        }
    },
