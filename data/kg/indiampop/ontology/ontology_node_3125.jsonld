{
    "@context": {
        "schema": "http://schema.org/",
        "cropCult": "http://crop-cultivation.org/",
        "Crop": {
            "@id": "cropCult:Crop",
            "@type": "schema:Thing",
            "@context": {
                "name": "schema:name",
                "has_types": "cropCult:CropType"
            }
        },
        "CropType": {
            "@id": "cropCult:CropType",
            "@type": "schema:Thing",
            "@context": {
                "name": "schema:name",
                "used_for": "schema:Text"
            }
        }
    },
    "@graph": [
        {
            "@type": "Crop",
            "name": "LIME (Citrus aurantifoilia Swing)",
            "has_types": [
                {
                    "@type": "CropType",
                    "name": "Sweet Lime",
                    "used_for": "Mithachikra, Mithotra"
                },
                {
                    "@type": "CropType",
                    "name": "Lemon",
                    "used_for": "Eureka, Lisbon, Assam Lemon, Nepali Round"
                }
            ]
        }
    ]
}