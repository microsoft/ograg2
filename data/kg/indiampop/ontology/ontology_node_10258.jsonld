            },
            "needs_disease_treatements": {
                "@type": "DiseaseTreatements",
                "disease_name": "Blast (Pyricularia oryzae)",
                "disease_symptoms": "PoP for Organic Farming24",
                "disease_control": {
                    "@type": "HerbicideList",
                    "name": "Bacterial blight Ooze test",
                    "quantity": "Cut the infected leaves and dip it in a glass of water and hold it against the light for few minutes without shaking. From the cut ends white milky bacterial ooze will come out as streams. This test confirms bacterial infection."
                }
            }
        }
    ]
}