            },
            "needs_pest_treatements": {
                "@type": "cropCult:PestTreatements",
                "pest_name": "Nitrogen",
                "pest_symptoms": "May damage the root system and affect survival of the plants if there is a dry period soon after planting.",
                "pest_control": {
                    "@type": "cropCult:PesticideList",
                    "name": "Nitrogen",
                    "quantity": "Not applied in the planting pits",
                    "stage": "After planting"
                }
            },
            "needs_disease_treatements": {
                "@type": "cropCult:DiseaseTreatements",
                "disease_name": "Mg deficiency",
                "disease_symptoms": "In Mg deficient soils",
                "disease_control": {
                    "@type": "cropCult:HerbicideList",
                    "name": "Magnesium",
                    "quantity": "100 g as anhydrous MgSO",
                    "stage": "After planting"
                }
            }
        }
    ]
}