{
    "@context": {
        "schema": "http://schema.org/",
        "cropCult": "http://crop-cultivation.org/",
        "Grading": {
            "@id": "cropCult:Grading",
            "@type": "schema:Thing",
            "@context": {
                "local_grades": "schema:List",
                "stem_length": "schema:QuantitativeValue",
                "yield": "schema:Text"
            }
        }
    },
    "@graph": [
        {
            "@type": "Grading",
            "local_grades": [
                {
                    "grade": "A",
                    "stem_length": "> 45 cm"
                },
                {
                    "grade": "B",
                    "stem_length": "< 45 cm > 30 cm"
                },
                {
                    "grade": "C",
                    "stem_length": "< 30 cm"
                }
            ],
            "yield": "Yield"
        }
    ]
}