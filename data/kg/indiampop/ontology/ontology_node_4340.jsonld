            "has_types": {
                "@type": "CropType",
                "name": "Seed Inoculation"
            },
            "SeedList": {
                "@type": "SeedList",
                "has_seeding_rate": {
                    "@type": "schema:QuantitativeValue",
                    "value": "75 kg/ha or 10 kg/bigha",
                    "description": "A seed rate of 75 kg/ha or 10 kg/bigha is required to obtain about 5 lakhs plants/ha (66,666 plants/bigha)."
                }
            }
        }
    ]
