{
    "@context": {
        "schema": "http://schema.org/",
        "cropCult": "http://crop-cultivation.org/",
        "SowingTime": {
            "@id": "cropCult:SowingTime",
            "@type": "schema:Thing",
            "@context": {
                "start_date": "schema:startDate",
                "end_date": "schema:endDate"
            }
        },
        "seedSpacing": {
            "@id": "cropCult:SeedSpacing",
            "@type": "schema:Thing",
            "@context": {
                "sowing_time": "schema:Text",
                "value": "schema:Text",
                "depth": "schema:Text",
                "method": "schema:Text"
            }
        }
    },
    "@graph": [
        {
            "@type": "SowingTime",
            "start_date": "mid October",
            "end_date": "mid November"
        },
        {
            "@type": "seedSpacing",
            "sowing_time": "Optimum time of sowing is mid October to mid November.",
            "depth": "5 cm"
        }
    ]
}