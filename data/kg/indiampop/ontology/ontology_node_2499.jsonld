{
    "@context": {
        "schema": "http://schema.org/",
        "cropCult": "http://crop-cultivation.org/",
        "Crop": {
            "@id": "cropCult:Crop",
            "@type": "schema:Thing",
            "@context": {
                "name": "schema:name",
                "has_types": "cropCult:CropType",
                "has_growing_zones": "cropCult:CropGrowingZones",
                "has_temperature_requirements": "cropCult:TemperatureRequirements",
                "has_soil_requirements": "cropCult:SoilRequirements",
                "has_land_preperation_requirements": "cropCult:LandPreparation",
                "seed_germination_test_requirements_are": "schema:Text",
                "has_seed_fungicide_requirements": "cropCult:Fungicide",
                "needs_seed_sowing_spacing": "cropCult:SeedSpacing",
                "weed_management_needs_are": "cropCult:WeedManagement",
                "needs_irrigation_schedule": "cropCult:IrrigationRequirement",
                "needs_disease_treatements": "cropCult:DiseaseTreatements",
                "needs_pest_treatements": "cropCult:PestTreatements",
                "harvesting_guidelines_are": "schema:Text",
                "storage_guidelines_are":"schema:Text"
            }
        },
        "SeedList": {
            "@id": "cropCult:SeedList",
            "@type": "schema:Thing",
            "@context": {
                "variety_name": "schema:List",
                "has_early_sowing_time": "cropCult:SowingTime",
                "has_late_sowing_time": "cropCult:SowingTime",
                "has_seeding_rate": "schema:QuantitativeValue",
                "has_fertilizer_application_requirements": "cropCult:Fertilization"
            }
        },
        "CropType": {
            "@id": "cropCult:CropType",
            "@type": "schema:Thing",
            "@context": {
                "name": "schema:name",
                "used_for": "schema:Text"
            }
        },
        "CropGrowingZones":{
            "@container": "@list",
            "@id": "cropCult:CropGrowingZones",
            "@type": "schema:ItemList",
            "CropGrowingZone": {
                "@id": "cropCult:CropGrowingZone",
                "@type": "schema:Thing",
                "@context": {
                    "name": "schema:name",
                    "has_regions": "schema:List",
                    "has_seed_recommendations": "cropCult:SeedList"
                }
            }
        },
        "TemperatureRequirements": {
            "@id": "cropCult:TemperatureRequirements",
            "@type": "schema:Thing",
            "@context": {
                "for_germination": "schema:Text",
                "for_flowering": "schema:Text",
                "for_grain_filling": "schema:Text",
                "negative_effect": "schema:Text"
            }
        },
        "SoilRequirements": {
            "@id": "cropCult:SoilRequirements",
            "@type": "schema:Thing",
            "@context": {
                "soil_type_name": "schema:Text",
                "avoid_soil_type": "schema:Text",
                "for_water": "schema:Text",
                "for_nutrients": "schema:Text"
            }
        },
        "SowingTime": {
            "@id": "cropCult:SowingTime",
            "@type": "schema:Thing",
            "@context": {
                "start_date": "schema:startDate",
                "end_date": "schema:endDate"
            }
        },
        "LandPreparation": {
            "@id": "cropCult:LandPreparation",
            "@type": "schema:Thing",
            "@context": {
                "instructions": "schema:Text",
                "with_soil_moisture": "schema:Text"
            }
        },
        "Fungicide": {
            "@id": "cropCult:Fungicide",
            "@type": "schema:Thing",
            "@context": {
                "name": "schema:Text",
                "quantity": "schema:Text"
            }
        },
        "SeedSpacing": {
            "@id": "cropCult:SeedSpacing",
            "@type": "schema:Thing",
            "@context": {
                "sowing_time": "schema:Text",
                "value": "schema:Text",
                "depth": "schema:Text",
                "method": "schema:Text"
            }
        },
        "Fertilization": {
            "@id": "cropCult:Fertilization",
            "@type": "schema:Thing",
            "@context": {
                "nutrient_name": "schema:Text",
                "quantity": "schema:Text",
                "stage_of_application": "schema:Text"
            }
        },
        "WeedManagement": {
            "@id": "cropCult:WeedManagement",
            "@type": "schema:Thing",
            "@context": {
                "weed_name": "schema:Text",
                "weed_types": "schema:List",
                "herbicides": "cropCult:HerbicideList"
            }
        },
        "IrrigationRequirement": {
            "@id": "cropCult:IrrigationRequirement",
            "@type": "schema:Thing",
            "@context": {
                "quantity": "schema:Text",
                "stage_of_application": "schema:Text"
            }
        },
        "HerbicideList": {
            "@id": "cropCult:HerbicideList",
            "@type": "schema:List",
            "@context": {
                "name": "schema:Text",
                "quantity": "schema:Text",
                "stage": "schema:Text"
            }
        },
        "PestTreatements": {
            "@id": "cropCult:PestTreatements",
            "@type": "schema:Thing",
            "@context": {
                "pest_name": "schema:Text",
                "pest_symptoms": "schema:Text",
                "pest_control": "cropCult:PesticideList"
            }
        },
        "PesticideList": {
            "@id": "cropCult:PesticideList",
            "@type": "schema:List",
            "@context": {
                "name": "schema:Text",
                "quantity": "schema:Text",
                "stage": "schema:Text"
            }
        },
        "DiseaseTreatements": {
            "@id": "cropCult:DiseaseTreatements",
            "@type": "schema:Thing",
            "@context": {
                "disease_name": "schema:Text",
                "disease_symptoms": "schema:Text",
                "disease_control": "cropCult:HerbicideList"
            }
        }
    },
    "@graph": [
        {
            "@type": "Crop",
            "name": "Banana",
            "has_types": {
                "@type": "CropType",
                "name": "Musa spp.",
                "used_for": "Fruits"
            },
            "has_land_preperation_requirements": {
                "@type": "LandPreparation",
                "instructions": "Select 3-4 months old disease free sword suckers from healthy clumps. Suckers should be removed one week after the harvest of the bunch. In the case of Nendran variety, cut back pseudostem to a length of 15-20 cm from corm and remove old roots. The portions of corm and roots infected by rhizome weevil (black tunnels) and nematode (darkened lesions) should be removed. The rhizome should be dipped in hot water (50Â°C) for 20 minutes to prevent nematode infestation. The rhizomes are to be smeared with cow dung solution and ash and dried in the sun for about 3-4 days and stored in shade up to 15 days before planting. Soaking the suckers in Pseudomonas fluorescens solution (2%) for 30 minutes before planting is beneficial.",
                "with_soil_moisture": "Not specified"
            },
            "has_seed_fungicide_requirements": {
                "@type": "Fungicide",
                "name": "Pseudomonas fluorescens",
                "quantity": "2%"
            }
        }
    ]
