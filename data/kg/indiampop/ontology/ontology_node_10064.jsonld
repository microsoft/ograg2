            }
        }
    },
    "@graph": [
        {
            "@type": "Crop",
            "name": "YAMS (Dioscorea spp.)",
            "has_land_preperation_requirements": {
                "@type": "LandPreparation",
                "instructions": "Shallow hoeing is done to provide better aeration for good plant growth and keeping down the weeds. Sometimes earthing-up is also done which help in giving support to the vines and also prevent water stagnation near to the base of the vine."
            },
            "weed_management_needs_are": {
                "@type": "WeedManagement",
                "weed_name": "Yams",
